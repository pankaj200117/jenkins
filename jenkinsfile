pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/pankaj200117/jenkins.git'
            }
        }

        stage('Build') {
            steps {
                sh 'echo "Building..."'
                // Add actual build commands here
            }
        }

        stage('Test') {
            steps {
                sh 'echo "Testing..."'
                // Add actual test commands here
            }
        }

        stage('Deploy') {
            steps {
                sh 'echo "Deploying..."'
                // Add actual deployment commands here
            }
        }
    }

    post {
        success {
            echo 'Pipeline succeeded!'
        }
        failure {
            echo 'Pipeline failed!'
        }
    }
}

